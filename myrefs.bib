}

@article{thaivalappilFoodSafetyOlder2020,
  title = {Food Safety and the Older Consumer: {{A}} Systematic Review and Meta-Regression of Their Knowledge and Practices at Home},
  author = {Thaivalappil, Abhinand and Young, Ian and Paco, Charles and Jeyapalan, Apiramy and Papadopoulos, Andrew},
  year = {2020},
